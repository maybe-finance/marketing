<?xml version="1.0" encoding="UTF-8"?>
<urlset xmlns="http://www.sitemaps.org/schemas/sitemap/0.9">
  <% if @page == 1 %>
    <url>
      <loc>https://maybefinance.com</loc>
    </url>
    <url>
      <loc>https://maybefinance.com/pricing</loc>
    </url>
    <url>
      <loc>https://maybefinance.com/features/tracking</loc>
    </url>
    <url>
      <loc>https://maybefinance.com/features/transactions</loc>
    </url>
    <url>
      <loc>https://maybefinance.com/features/budgeting</loc>
    </url>
    <url>
      <loc>https://maybefinance.com/features/assistant</loc>
    </url>

    <url>
      <loc>https://maybefinance.com/privacy</loc>
    </url>
    <url>
      <loc>https://maybefinance.com/tos</loc>
    </url>
    <url>
      <loc>https://maybefinance.com/financial-terms</loc>
    </url>
    <% @terms.each do |term| %>
      <url>
        <loc>https://maybefinance.com/financial-terms/<%= term.slug %></loc>
      </url>
    <% end %>
    <url>
      <loc>https://maybefinance.com/articles</loc>
    </url>
    <% @articles.each do |article| %>
      <url>
        <loc>https://maybefinance.com/articles/<%= article.slug %></loc>
      </url>
    <% end %>
    <url>
      <loc>https://maybefinance.com/tools</loc>
    </url>
    <% @tools.each do |tool| %>
      <url>
        <loc>https://maybefinance.com/tools/<%= tool.slug %></loc>
      </url>
    <% end %>
    <% @exchange_rate_currencies.each do |from_currency| %>
      <% @exchange_rate_currencies.each do |to_currency| %>
        <% next if from_currency.last == to_currency.last # Skip same currency pairs %>
        <url>
          <loc>https://maybefinance.com/tools/exchange-rate-calculator/<%= from_currency.last %>/<%= to_currency.last %></loc>
        </url>
      <% end %>
    <% end %>
  <% end %>
  <url>
    <loc>https://maybefinance.com/tools/inside-trading-tracker/top-owners</loc>
  </url>
  <url>
    <loc>https://maybefinance.com/tools/inside-trading-tracker/biggest-trades</loc>
  </url>
  <url>
    <loc>https://maybefinance.com/tools/inside-trading-tracker/top-officers</loc>
  </url>
</urlset>

